[run]
branch = True
source = .

[report]
exclude_lines =
    if self.debug:
    pragma: no cover
    raise NotImplementedError
    if __name__ == .__main__.:
ignore_errors = True
omit =
    # Ignore test files
    */test*.py

    # Ignore migration related files
    */migrations/*

    # Ignore virtualenv in local
    venv/*

    # Ignore Django own python script
    manage.py
    homiehomie/wsgi.py
    homiehomie/asgi.py